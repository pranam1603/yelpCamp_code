<% layout('layouts/boilerplate') %>
    <h1>All Campground</h1>
    <a href="/campground/new">New Campground</a>
    <ul>
        <% for(let campground of campgrounds){ %>
            <li>
                <a href="/campground/<%=campground.id%>">
                    <%=campground.title%>
                </a>
            </li>
            <%}%>
    </ul>